{
  "compileOnSave": false,
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "downlevelIteration": true,
    "importHelpers": true,
    "module": "es2020",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2017", "dom"],
    "paths": {
      "ngx-ui-tour-core": ["projects/ngx-ui-tour-core/src/public_api"],
      "ngx-ui-tour-console": ["projects/ngx-ui-tour-console/src/public_api"],
      "ngx-ui-tour-md-menu": ["projects/ngx-ui-tour-md-menu/src/public_api"],
      "ngx-ui-tour-ng-bootstrap": ["projects/ngx-ui-tour-ng-bootstrap/src/public_api"],
      "ngx-ui-tour-ngx-bootstrap": ["projects/ngx-ui-tour-ngx-bootstrap/src/public_api"]
    }
  }
}
